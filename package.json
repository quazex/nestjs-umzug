{
    "name": "@quazex/nestjs-umzug",
    "repository": {
        "type": "git",
        "url": "git://github.com/quazex/nestjs-knex.git"
    },
    "description": "NestJS wrapper for Umzug migration tool",
    "version": "1.0.0",
    "author": "Alexander Smirnov",
    "license": "MIT",
    "engines": {
        "node": ">= 20.x"
    },
    "publishConfig": {
        "access": "public"
    },
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "files": [
        "lib/**"
    ],
    "keywords": [
        "migrations",
        "pg",
        "nestjs",
        "nest",
        "typescript",
        "nodejs",
        "node"
    ],
    "scripts": {
        "build:clear": "rimraf lib",
        "build:compile": "npm run build:clear && tsc --project tsconfig.build.json",
        "dev:watch": "tsc --watch",
        "dev:check": "tsc --noEmit",
        "dev:lint": "eslint ./source --ext .ts",
        "dev:format": "npm run dev:lint -- --fix",
        "dev:test": "jest",
        "commit": "git-cz",
        "release": "semantic-release"
    },
    "peerDependencies": {
        "@nestjs/common": ">=9.0.0 <11.0.0",
        "@nestjs/core": ">=9.0.0 <11.0.0"
    },
    "optionalDependencies": {
        "pg": ">= 8.0.0"
    },
    "dependencies": {
        "dedent": "^1.6.0",
        "nest-commander": "^3.17.0",
        "umzug": "^3.8.2",
        "uuid": "^11.1.0"
    },
    "devDependencies": {
        "@faker-js/faker": "9.6.0",
        "@jest/globals": "29.7.0",
        "@nestjs/common": "10.4.15",
        "@nestjs/testing": "10.4.15",
        "@quazex/eslint-config": "1.0.2",
        "@quazex/nestjs-knex": "1.0.0",
        "@semantic-release/git": "10.0.1",
        "@testcontainers/postgresql": "10.23.0",
        "@types/pg": "8.15.4",
        "commitizen": "4.3.1",
        "cz-conventional-changelog": "3.3.0",
        "husky": "9.1.7",
        "jest": "29.7.0",
        "knex": "3.1.0",
        "pg": "8.16.0",
        "rimraf": "6.0.1",
        "semantic-release": "24.2.3",
        "testcontainers": "10.22.0",
        "ts-jest": "29.3.0",
        "ts-node": "10.9.2",
        "type-fest": "4.30.0",
        "typescript": "5.8.2"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
